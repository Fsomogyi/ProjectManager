@model IEnumerable<BusinessLogicLayer.Project>

@{
    ViewBag.Title = "Index";
}

<h2>My projects</h2>


<div class="container">
    @foreach (var item in Model)
    {
        <div class="project-element">
            <p class="project-title">@Html.ActionLink(item.Name, "Details", "Projects", routeValues:  new { id = @item.Id }, htmlAttributes: null)</p>
            @item.Deadline
        </div>
    }
</div>
